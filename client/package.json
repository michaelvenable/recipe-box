{
  "name": "recipe-box",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^1.2.3",
    "dexie": "^3.2.2",
    "font-awesome": "^4.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.7.0",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "prebuild": "npm run build-recipe-file",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "build-recipe-file": "node ../scripts/build-recipe-file.js",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-restricted-globals": [
        "error",
        "event",
        "fdescribe"
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
